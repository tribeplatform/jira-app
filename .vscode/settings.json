{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[typescript]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": false,
      "source.fixAll.prettier": true,
      "source.fixAll.eslint": true
    }
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.preferences.importModuleSpecifier": "relative",
  "editor.formatOnPaste": false,
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "file",
  "eslint.format.enable": true,
  "cSpell.words": [
    "acks",
    "anonymization",
    "backoff",
    "Bizops",
    "Blocklist",
    "chakra",
    "cobertura",
    "codemirror",
    "Customizer",
    "dataloader",
    "dind",
    "dompurify",
    "dtos",
    "favicon",
    "fullstory",
    "fulltext",
    "gitlab",
    "Gitops",
    "hubspot",
    "Imgix",
    "interruptible",
    "jsonb",
    "KEYSPACE",
    "kubernetes",
    "linkify",
    "linkifyjs",
    "liquidjs",
    "localstack",
    "LZUTF",
    "metatype",
    "Moderations",
    "nestjs",
    "ngram",
    "numpy",
    "openapi",
    "postgres",
    "Redirections",
    "Repliable",
    "requisitive",
    "SENDGRID",
    "Ssos",
    "Timeframe",
    "tribeplatform",
    "typeorm",
    "Unfollow",
    "UNFOLLOWED",
    "unpublish",
    "upsert",
    "varchar",
    "Whitespaces"
  ],
  "cassandraWorkbench.activeClusterName": "Local",
  "cassandraWorkbench.activeKeyspace": [["Local", "streams"]],
  "vsicons.presets.nestjs": true,
  "liquid.rules": {
    "ignore": [
      {
        "type": "liquid",
        "begin": "comment",
        "end": "endcomment"
      },
      {
        "type": "html",
        "begin": "script",
        "end": "script"
      },
      {
        "type": "html",
        "begin": "style",
        "end": "style"
      }
    ],
    "html": {
      "correct": false,
      "force_attribute": false,
      "braces": false,
      "preserve": 2,
      "unformatted": false
    },
    "js": {
      "preserve": 1,
      "method_chain": 0,
      "quote_convert": "none",
      "format_array": "indent",
      "format_object": "indent",
      "braces": false,
      "no_semicolon": false,
      "brace_block": false
    },
    "scss": {
      "css_insert_lines": true,
      "preserve": 2,
      "braces": false,
      "brace_block": true
    },
    "css": {
      "css_insert_lines": true,
      "preserve": 2,
      "braces": false,
      "brace_block": true
    },
    "json": {
      "preserve": 0,
      "braces": false,
      "brace_block": true,
      "no_semicolon": true
    }
  }
}
